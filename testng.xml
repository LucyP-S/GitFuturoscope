<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Regression Suite">

    <listeners>
        <listener class-name="testcomponents.Listeners"/>
    </listeners>
    <test thread-count="5" name="Booking Flow Tests">
        <classes>
            <class name="tests.BookingFlow"/>
        </classes>
    </test>

    <test thread-count="5" name="Error Validations Test">
        <classes>
            <class name="tests.ErrorsValidation"/>
        </classes>
    </test>
</suite>